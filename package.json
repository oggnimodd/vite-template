{
  "name": "bun-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pre-commit": "bun lint-staged",
    "ladle:dev": "ladle serve",
    "ladle:build": "ladle build",
    "ladle:preview": "ladle preview",
    "lint": "biome check .",
    "lint:apply": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "prepare": "bun prepare.js"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@ladle/react": "4.0.0",
    "@nextui-org/react": "^2.1.13",
    "@vitejs/plugin-react": "^4.2.0",
    "framer-motion": "^10.16.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lucide-react": "^0.293.0",
    "nanoid": "^5.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "latest",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.4"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "biome check --apply .",
      "biome format --write ."
    ]
  }
}